{
    "name": "10quality/php-comprobante-electronico-costa-rica",
    "description": "PHP client used to generate and send online invoices to Costa Rica's 'Ministerio de Hacienda' (Costa Rica's IRS)",
    "license": "MIT",
    "homepage": "https://github.com/10quality/php-comprobante-electronico-costa-rica",
    "keywords": [
        "php",
        "comprobante electronico",
        "factura electronica",
        "costa rica",
        "hacienda"
    ],
    "type": "library",
    "authors": [
        {
            "name": "10 Quality",
            "email": "info@10quality.com"
        }
    ],
    "autoload": {
        "files": [
            "src/Lib/functions.php"
        ],
        "psr-4": {
            "ComprobanteElectronico\\": "src"
        }
    },
    "require": {
        "php": ">=5.5",
        "10quality/php-data-model": "1.0.*",
        "10quality/php-curl": "1.0.*",
        "philipp15b/php-i18n": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8"
    }
}
